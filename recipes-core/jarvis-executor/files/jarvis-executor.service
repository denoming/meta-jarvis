[Unit]
Description=J.A.R.V.I.S executor service
Wants=jarvis-speaker.service jarvis-weather.service
After=jarvis-speaker.service jarvis-weather.service

[Service]
Type=simple
ExecStart=/usr/bin/jarvis-executor
Environment=JARVIS_EXECUTOR_CONFIG=/usr/share/jarvis/jarvis-executor-config.json

[Install]
WantedBy=multi-user.target
